{
  "name": "xanpool-full-stack-assignment",
  "version": "1.0.0",
  "description": "This application is for techical assessment.",
  "main": "back-end/build/index.js",
  "types": "back-end/build/index.d.ts",
  "scripts": {
    "api:prod:start": "npm run api:build && ts-node back-end/build/src/index.js",
    "api:dev:start": "env-cmd -f .env ts-node-dev back-end/src/index.ts",
    "api:db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "api:dbmigrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
    "api:build": "env-cmd -f .env rimraf back-end/build && tsc -p back-end/tsconfig.json",
    "tslint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different",
    "prettier:write": "npm run prettier:base -- --write",
    "client:start": "webpack serve --config webpack.config.js",
    "client:build": "webpack serve --config webpack.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-session": "^2.0.42",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^16.11.6",
    "@types/react-bootstrap": "^0.32.30",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^13.6.6",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.1.3",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "interpolate-html-plugin": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "sequelize": "^6.9.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^4.41.32",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.7.4",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "back-end/**/*.{ts,js}": [
      "npm run prettier:check",
      "npm run prettier:write",
      "npm run tslint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
